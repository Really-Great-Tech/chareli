{
  "family": "chareli-task-development-v2",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::330858616968:role/chareli-ecs-task-execution-development-v2",
  "taskRoleArn": "arn:aws:iam::330858616968:role/chareli-ecs-task-role-development-v2",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "containerDefinitions": [
    {
      "name": "chareli-ecs-backend-development-server-v2",
      "image": "PLACEHOLDER_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/chareli-development-v2",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
